{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "RamuADFactory"
		},
		"AzureDatabricksLinkedService_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricksLinkedService'"
		},
		"AzureDatabricksLinkedService_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0120-115431-ogwa01lf"
		},
		"trigger2_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/ed99c131-036e-4866-ade1-53997fd31720/resourceGroups/RG1/providers/Microsoft.Storage/storageAccounts/ramusa1"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "RamuADFactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDatabricksLinkedService_accessToken": {
						"value": "[parameters('AzureDatabricksLinkedService_accessToken')]"
					},
					"AzureDatabricksLinkedService_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricksLinkedService_properties_typeProperties_existingClusterId')]"
					},
					"trigger2_properties_typeProperties_scope": {
						"value": "[parameters('trigger2_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}